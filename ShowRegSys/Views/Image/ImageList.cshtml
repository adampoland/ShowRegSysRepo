@model IEnumerable<ShowRegSys.Models.Image>

@{
    ViewBag.Title = "ImageList";
}

<h2>ImageList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
@foreach (var item in Model)
{

    <tr>
        <a href="@Url.Action("SingleImage", new { imageId = item.ImageId, showId = item.ShowId })"><img src="@(item.Path)" width="200" /></a>
 
    </tr>

}
</table>